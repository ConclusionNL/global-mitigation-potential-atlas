{
    "name": "arlanet.frontend.library",
    "version": "0.3.0",
    "author": "Arlanet",
    "type": "module",
    "engines": {
        "npm": ">=8.19.2",
        "node": ">=18.10.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest --dom",
        "eslint:check": "eslint -c .eslintrc.json ./**/*.{js,ts,vue} --cache --cache-location ./dev-reports/.eslintcache",
        "eslint:fix": "eslint -c .eslintrc.json ./**/*.{js,ts,vue} --cache --cache-location ./dev-reports/.eslintcache --fix",
        "prettier:check": "prettier --config prettier.config.js --check --debug-check ./**/*.{js,ts,vue,scss,html}",
        "prettier:fix": "prettier --config prettier.config.js ./**/*.{js,ts,vue,scss,html} --write --cache --log-level silent",
        "stylelint": "stylelint --config .stylelintrc.json ./**/*.scss --formatter verbose",
        "stylelint:fix": "stylelint --config .stylelintrc.json ./**/*.scss --formatter verbose --fix",
        "svg:all": "npm run svg:optimize && npm run svg:sprite",
        "svg:optimize": "svgo -f ./sprites -o ./sprites --config .svgorc.js",
        "svg:sprite": "svg-sprite sprites/*.svg --config .svgspriterc.json",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "preclean": "npm run del",
        "clean": "npm install",
        "postclean": "echo All node modules are re-installed! Happy coding !!!",
        "del": "npx rimraf node_modules package-lock.json"
    },
    "devDependencies": {
        "@intlify/unplugin-vue-i18n": "^0.11.0",
        "@pinia/testing": "^0.1.2",
        "@rollup/plugin-dsv": "^3.0.2",
        "@storybook/addon-essentials": "^7.0.20",
        "@storybook/addon-interactions": "^7.0.20",
        "@storybook/addon-links": "^7.0.20",
        "@storybook/addon-mdx-gfm": "^7.0.20",
        "@storybook/addon-styling": "^1.3.2",
        "@storybook/blocks": "^7.0.20",
        "@storybook/testing-library": "^0.1.0",
        "@storybook/vue3": "^7.0.20",
        "@storybook/vue3-vite": "^7.0.20",
        "@tsconfig/node18": "^18.2.0",
        "@types/node": "^18.17.5",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vitest/coverage-istanbul": "^0.33.0",
        "@vitest/coverage-v8": "^0.32.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.4.1",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.46.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-storybook": "^0.6.13",
        "eslint-plugin-vue": "^9.16.1",
        "gh-pages": "^6.0.0",
        "glob": "^8.0.3",
        "postcss": "^8.4.24",
        "prettier": "^3.0.0",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^4.4.0",
        "sass": "^1.62.1",
        "storybook": "^7.0.20",
        "stylelint": "^14.16.1",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-csstree-validator": "^2.0.0",
        "stylelint-sass-render-errors": "^3.0.0",
        "stylelint-selector-bem-pattern": "^2.1.1",
        "svg-sprite": "^2.0.2",
        "svgo": "^2.8.0",
        "tailwindcss": "^3.3.2",
        "typescript": "^4.8.4",
        "vite": "^4.3.9",
        "vitest": "^0.32.0",
        "vue-tsc": "^1.8.8"
    },
    "dependencies": {
        "@vueuse/core": "^10.1.2",
        "bootstrap": "^5.3.0",
        "d3": "^6.7.0",
        "d3-geo": "^3.1.0",
        "d3-geo-projection": "^4.0.0",
        "d3-scale": "^4.0.2",
        "happy-dom": "^10.5.2",
        "normalize.css": "^8.0.1",
        "pinia": "^2.1.3",
        "primevue": "^3.31.0",
        "rfs": "^10.0.0",
        "swiper": "^9.2.0",
        "topojson": "^3.0.2",
        "topojson-client": "^3.1.0",
        "vite-svg-loader": "^4.0.0",
        "vue": "^3.3.4",
        "vue-i18n": "^9.2.2"
    }
}
